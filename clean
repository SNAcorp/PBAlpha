from RFIDReader import RFIDReader
from ButtonReader import ButtonReader
from DrinkDispenser import DrinkDispenser
from ReciptModel import Recipt
from SessionLoader import SessionMonitor


class ProgramExitManager:
    def __init__(self, but_clean, __clean, end_read):
        self.clean = but_clean
        self.clean_res = __clean
        self.clean_RFID = end_read

    def clean_RFID(self):
        self.clean_RFID.end_read

    def clean_ButtonReader(self):
        self.clean_ButtonReader.clean

    def clean_Recipt(self):
        self.clean_Recipt.clean_res
